{
	"name": "myext",
	"displayName": "my extension",
	"description": "vscode extension template",
	"version": "1.0.0",
	"publisher": "yor name",
	"private": true,
	"license": "ISC",
	"repository": {
		"type": "git",
		"url": "https://github.com/"
	},
	"engines": {
		"vscode": "^1.75.0"
	},
	"categories": [
		"Programming Languages"
	],
	"main": "./out/extension.js",
	"icon": "images/icon.png",
	"activationEvents": [
		"onLanguage:myext"
	],
	"contributes": {
		"languages": [
			{
				"id": "myext",
				"extensions": [
					".ext"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "mySidebar",
					"title": "My Sidebar",
					"icon": "./images/sidebar.png"
				},
				{
					"id": "myWebview",
					"title": "My Sidebar WebView",
					"icon": "./images/sidebar.png"
				}
			]
		},
		"views": {
			"mySidebar": [
				{
					"id": "mySidebar",
					"name": "test"
				}
			],
			"myWebview": [
				{
					"type": "webview",
					"id": "mySidebarWebView",
					"name": "test"
				}
			]
		},
		"commands": [
			{
				"command": "mySidebar.myext",
				"title": "MyExtension",
				"icon": "./images/sidebarToolIcon.png"
			},
			{
				"command": "mySidebarWebView.myext",
				"title": "MyExtension2",
				"icon": "./images/sidebarToolIcon.png"
			}
		],
		"menus": {
			"editor/context": [
				{
					"when": "resourceLangId == 'myext' && resourceExtname == '.ext'",
					"command": "mySidebar.myext",
					"group": "debug"
				}
			],
			"view/title": [
				{
					"command": "mySidebar.myext",
					"when": "view == mySidebar",
					"group": "navigation"
				},
				{
					"command": "mySidebarWebView.myext",
					"when": "view == mySidebarWebView",
					"group": "navigation"
				}
			]
		},
		"configuration": {
			"title": "MyExt",
			"properties": {
				"MyExt.textSetting": {
					"type": "string",
					"default": "MyExt Sample Text",
					"description": "MyExt Test Setting"
				},
				"MyExt.booleanSetting": {
					"type": "boolean",
					"default": true,
					"description": "MyExt Boolean Setting"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./ && gulp",
		"lint": "eslint . --ext .ts,.tsx",
		"watch": "tsc -watch -p ./",
		"gulp": "gulp",
		"package": "vsce package"
	},
	"devDependencies": {
		"@types/vscode": "^1.80.0",
		"@types/node": "20.2.5",
		"@typescript-eslint/eslint-plugin": "^4.16.0",
		"@typescript-eslint/parser": "^4.16.0",
		"eslint": "^7.21.0",
		"gulp": "^4.0.2",
		"gulp-terser": "^2.1.0",
		"gulp-typescript": "^6.0.0-alpha.1",
		"vsce": "^2.15.0"
	},
	"dependencies": {
		"ts-node": "^10.9.1",
		"tsconfig-paths": "^4.1.2",
		"typescript": "^4.9.4"
	}
}
