{
    "comments": {
        // 単一行のコメントに使用される記号。言語が行コメントをサポートしていない場合は、このエントリを削除してください
        "lineComment": "//",
        // ブロックコメントの開始と終了に使用される記号。言語がブロック コメントをサポートしていない場合は、このエントリを削除してください
        "blockComment": [ "/*", "*/" ]
    },
    // 括弧として使用される記号
    "brackets": [
        ["{", "}"],
        ["[", "]"],
        ["(", ")"]
    ],
    // 入力時に自動的に閉じられる記号
    "autoClosingPairs": [
        ["{", "}"],
        ["[", "]"],
        ["(", ")"],
        ["\"", "\""],
        ["`", "`"],
        ["'", "'"],
        ["/**", " */"]
    ],
    // 選択範囲を囲むために使用できる記号
    "surroundingPairs": [
        ["{", "}"],
        ["[", "]"],
        ["(", ")"],
    ]
}